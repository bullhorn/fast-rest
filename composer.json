{
  "name": "bullhorn/fast-rest",
"minimum-stability": "dev",
"prefer-stable": true,
  "type": "library",
  "description": "FastREST generates REST-ready models and controllers dynamically from a MySQL database schema, including foreign keys, table and column comments, and indexes.",
  "keywords": [
    "framework",
    "phalcon",
    "rest",
	"api"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Bullhorn",
      "email": "fast-rest@bullhorn.com"
    }
  ],
  "require": {
    "php": ">=7.1.5",
    "ext-phalcon": ">=2.0.4 <3.9",
    "zircote/swagger-php": "*",
    "nicklewis/phalcon-db-mock": "dev-master",
    "bullhorn/fast-rest-services": "dev-BBO-4463",
    "riverline/multipart-parser": "^2.0"
  },
  "require-dev": {
    "phalcon/incubator": ">=2.0",
  "phpunit/phpunit": "^7",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "Bullhorn\\FastRest": "Library/"
    }
  }
}
